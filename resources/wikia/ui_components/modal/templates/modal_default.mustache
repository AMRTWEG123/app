<div id="{{id}}" class="modalWrapper {{#classes}}{{.}} {{/classes}}">
	<header>
		{{title}}
		{{#closeButton}}
			<a href="">Close</a>
		{{/closeButton}}
	</header>
	<section>
		{{{content}}}
	</section>
	<footer>
		{{#altLink}}
			<a id="{{altLink.id}}" href="{{altLink.href}}">{{altLink.text}}</a>
		{{/altLink}}
		{{{secondBtn}}}
		{{{primaryBtn}}}
	</footer>
</div>
