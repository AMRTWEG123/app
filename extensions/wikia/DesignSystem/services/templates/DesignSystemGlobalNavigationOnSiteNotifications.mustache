{{#.}}
    <a href="{{uri}}" class="wds-notification-card">
        <a>
            <div class="wds-notification-card__icon-wrapper">
                <svg class="wds-icon" viewBox="0 0 24 24">
                    <use xmlns:xlink="http://www.w3.org/1999/xlink"
                         xlink:href="#wds-icons-reply-small"></use>
                </svg>
            </div>
        </a>


        <div class="notification-body">
            {{text}}
            {{snippet}}
        </div>
    </a>
{{/.}}
