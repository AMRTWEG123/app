<section id="affiliate-module-{{option}}" class="affiliate-module {{className}}">
	<h1>{{title}}</h1>
	{{#products}}
	<div class="affiliate">
		<div>
			<a class="prod-thumb" target="_blank" href="{{url}}">
				<img alt="" src="{{thumbUrl}}">
			</a>
		</div>
		<div class="placard">
			<a class="prod-name" target="_blank" href="{{url}}" title="{{name}}">{{name}}</a>
		</div>
		<ul class="vendor">
			<li>
				<a class="vendor-logo" target="_blank" href="{{url}}"></a>
			</li>
			<li>
				<a class="wikia-button vendor-see" target="_blank" href="{{url}}">{{buttonLabel}}</a>
			</li>
			<li>
				<a class="vendor-price" target="_blank" href="{{url}}">{{price}}</a>
			</li>
		</ul>
	</div>
	{{/products}}
</section>
