<!doctype html>
<html lang="{{lang}}" dir="{{dir}}">
<head>
	<meta http-equiv="Content-Type" content="{{mimeType}}; charset={{charset}}">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
	{{{headLinks}}}
	<title>{{pageTitle}}</title>
	{{{cssLinks}}}
	{{{pageCss}}}
	{{{jsHeadScripts}}}
	{{{headItems}}}
</head>
<body class="{{bodyClasses}}">
<div id="ad-skin" class="wikia-ad noprint"></div>
{{{globalNavigation}}}
<div class="wikia-page-wrapper">
	<div class="top-ads">
		<div class="ad">
			{{{adTopLeaderboard}}}
		</div>
		{{{adInvisibleSkin}}}
	</div>
	<section class="wikia-page page-width">
		{{{localNavigation}}}
		<article class="article clearfix" id="WikiaArticle">
			{{{header}}}
			<div class="top-right-ad">
				<div class="ad">
					{{{adTopRightBoxad}}}
				</div>
			</div>
			<!-- Infobox placeholder for prototyping only -->
			<div class="infobox-test">Infobox</div>
			{{{leftArticleNav}}}
			{{{contents}}}
		</article>
		{{{globalFooter}}}
		{{! TODO: Re-Enable when it's ready: corporateFooter}}
	</section>
</div>
{{{adsBottom}}}
{{{jsBodyFiles}}}
</body>
</html>
