<section class="content-review-module module">
	<h4 class="content-review-module-header">{{headerLatest}}</h4>
	{{{latestStatus}}}
	<h4 class="content-review-module-header">{{headerLast}}</h4>
	{{{lastStatus}}}
	<h4 class="content-review-module-header">{{headerLive}}</h4>
	{{{liveStatus}}}

	{{#displaySubmit}}
	<div class="content-review-module-submit-container">
		<a href="#" id="content-review-module-submit" title="{{submit}}">
		<button class="content-review-module-button primary" type="button">
			{{submit}}
		</button>
		</a>
	</div>
	{{/displaySubmit}}

	<div class="content-review-module-test-mode">
		{{#isTestModeEnabled}}
		<button id="content-review-module-disable-test-mode" class="secondary">
			{{disableTestMode}}
		</button>
		{{/isTestModeEnabled}}
		{{^isTestModeEnabled}}
		<button id="content-review-module-enable-test-mode" class="secondary">
			{{enableTestMode}}
		</button>
		{{/isTestModeEnabled}}
	</div>
</section>
