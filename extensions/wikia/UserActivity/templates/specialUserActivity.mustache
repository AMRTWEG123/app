<h2>User Activity</h2>
<table style="padding:10px;" class="TablePager" id="useractivity-table">
	<thead>
<tr><th>Title</th><th># of edits</th><th>Last edited</th><th>User rights</th>
	</thead>
{{#items}}
<tr>
	<td>{{#wordmarkData}}{{> wordmark}}{{/wordmarkData}}{{^wordmarkData}}{{/wordmarkData}}
		<strong><a href="{{url}}">{{title}}</a></strong></td>
	<td><a href="{{url}}wiki/Special:Contributions/{{username}}">{{editString}}</a></td>
	<td>{{lastEdit}}</td>
	<td>{{groups}}</td>
</tr>
{{/items}}
</table>
{{{pagination}}}
