<div class="input-group">
    <select name="pinTypeParents[]">
        <option value="">{{emptyOption}}</option>
        {{#parentPinTypes}}
            {{> parentPinType}}
        {{/parentPinTypes}}
    </select>
	<input type="text" name="pinTypeNames[]" placeholder="{{placeholder}}" value="{{name}}">
	<a href="" title="{{delete}}" class="deleteIcon modalEvent" data-event="deletePinType">{{delete}}</a>
</div>
