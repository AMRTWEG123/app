{{#showAd}}
<!-- BEGIN SLOTNAME: {{slotName}} -->
<div id="{{slotName}}" class="wikia-ad noprint default-height">
    {{#includeLabel}}
    <label for="{{slotName}}" class="wikia-ad-label">{{msgAdEngineAdvertisement}}</label>
    {{/includeLabel}}
    {{#addToAdQueue}}
    <script>
        {{#onLoad}}
            wgAfterContentAndJS.push(function () {
                window.adslots2.push({{{slotNameJson}}});
            });
        {{/onLoad}}
        {{^onLoad}}
            window.adslots2.push({{{slotNameJson}}});
        {{/onLoad}}
    </script>
    {{/addToAdQueue}}
</div>
<!-- END SLOTNAME: {{slotName}} -->
{{/showAd}}
{{^showAd}}
<!-- NO AD {{slotName}} (levels: {{pageTypesJson}}) -->
{{/showAd}}
