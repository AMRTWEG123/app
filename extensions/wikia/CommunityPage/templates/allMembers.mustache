<h2>{{allMembersHeaderText}}</h2>
<ul class="reset top-contributors">
	{{#members}}
		<li class="community-page-contributor">
			<div class="avatar-container">
				<a href="{{profilePage}}">{{{avatar}}}</a>
			</div>
			<div class="community-page-contributor-details">
				<a href="{{profilePage}}">{{userName}}</a>
				{{#isAdmin}}
					<span class="community-page-subtle">{{admin}}</span>
				{{/isAdmin}}
				<span class="community-page-details">{{joinedText}} {{joinDate}}</span>
			</div>

		</li>
	{{/members}}
	{{^members}}
		<div class="community-page-zero">
			{{noMembersText}}
		</div>
	{{/members}}
</ul>
