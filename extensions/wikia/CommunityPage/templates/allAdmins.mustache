<h2>{{allMembersHeaderText}}</h2>
<ul class="reset top-contributors">
	{{#admins}}
		<li>
			<div class="avatar-container">
				<a href="{{profilePage}}">{{{avatar}}}</a>
			</div>
			<div class="contributor-details">
				<span class="details">{{contributionsText}}</span>
				<a href="{{profilePage}}">{{userName}}</a>
				{{#isAdmin}}
					<span class="subtle">{{admin}}</span>
				{{/isAdmin}}
			</div>
		</li>
	{{/admins}}
	{{^admins}}
		<div class="zero">
			{{noAdminText}}
			<a href="{{noAdminHref}}">{{noAdminContactText}}</a>
		</div>
	{{/admins}}
</ul>
