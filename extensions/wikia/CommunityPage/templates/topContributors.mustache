<h2>{{topContribsHeaderText}}</h2>
<div class="user-details">
	<div class="avatar-container">
		{{{userAvatar}}}
	</div>
	<div class="community-page-rank">
		<span>{{userRank}} <small>/ {{weeklyEditorCount}}</small></span>
		<span class="community-page-subtle">{{yourRankText}}</span>
	</div>
	<div class="user-contrib-count">
		<span>{{userContribCount}}</span>
		<span class="community-page-subtle">{{userContributionsText}}</span>
	</div>
</div>
<ul class="reset top-contributors">
	{{#contributors}}
		<li class="community-page-contributor">
			<div class="avatar-container">
				<a href="{{profilePage}}">{{{avatar}}}</a>
			</div>
			<div class="community-page-contributor-details">
				{{count}}. <a href="{{profilePage}}">{{userName}}</a>
				<span class="community-page-subtle">{{contributionsText}}</span>
			</div>

		</li>
	{{/contributors}}
	{{^contributors}}
		<div class="community-page-zero">
			{{noContribsText}}
		</div>
	{{/contributors}}
</ul>
