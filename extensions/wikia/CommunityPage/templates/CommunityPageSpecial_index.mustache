{{#showPopupMessage}}
	<div class="community-page-message">
		<div class="info-icon"></div>
		<div class="text">
			{{popupMessageText}}
		</div>
		<div class="close-icon"></div>
	</div>
{{/showPopupMessage}}

{{#userIsMember}}<p>Current User is a Member of this Wiki</p>{{/userIsMember}}
{{^userIsMember}}<p>Current User is not a member or is Anon</p>{{/userIsMember}}

{{#contributors}}
	<p>{{{avatar}}} <a href="{{profilePage}}">{{userName}}</a><br>
		{{contributions}} contributions</p>
{{/contributors}}