{{#hasUserError}}
	<p>{{userErrorMessage}}</p>
{{/hasUserError}}
{{^hasUserError}}
	{{#hasNoUserLogRecords}}
		<p>{{noUserLogRecordsMessage}}</p>
	{{/hasNoUserLogRecords}}
	{{^hasNoUserLogRecords}}
		<div class="log-table-caption">
            <p>{{logTableCaption}}</p>
        </div>
		<table class="article-table">
			<thead>
			<tr>
				<th>{{userNameHeader}}</th>
				<th>{{ipAddressHeader}}</th>
				<th>{{locationHeader}}</th>
				<th>{{languageHeader}}</th>
				<th>{{userAgentHeader}}</th>
				<th>{{appHeader}}</th>
				<th>{{timestampHeader}}</th>
			</tr>
			</thead>
			{{#userLogRecords}}
				<tr>
					<td>{{userName}}</td>
					<td>{{ip}}</td>
					<td>{{location}}</td>
					<td>{{language}}</td>
					<td>{{userAgent}}</td>
					<td>{{app}}</td>
					<td>{{timestamp}}</td>
				</tr>
			{{/userLogRecords}}
		</table>
	{{/hasNoUserLogRecords}}
{{/hasUserError}}
